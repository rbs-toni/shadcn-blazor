@namespace ShadcnBlazor

<CascadingValue Value="this">
    <CascadingValue Value="_globalDialogOptions">
        @foreach (var modal in _modals)
        {
            @modal.DialogInstance
        }
    </CascadingValue>
</CascadingValue>

@code {
    [CascadingParameter] 
    IDialogService CascadedDialogService { get; set; } = default!;
}